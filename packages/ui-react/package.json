{
  "name": "ui-react",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently 'npm:dev:*'",
    "dev:vite": "vite",
    "dev:storybook": "storybook dev -p 6006",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "lint:eslint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:prettier": "prettier --write .",
    "test": "NODE_ENV=test jest",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:watch": "NODE_ENV=test jest --coverage --watchAll",
    "build": "npm run build:vite && npm run build:storybook",
    "build:vite": "tsc && vite build",
    "build:storybook": "storybook build",
    "preview": "vite preview",
    "clean": "rimraf ./dist && rimraf ./storybook-static"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^3.1.0",
    "classnames": "^2.3.2",
    "i18next": "^22.5.0",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-i18next": "^12.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.0.14",
    "@storybook/addon-essentials": "^7.0.14",
    "@storybook/addon-interactions": "^7.0.14",
    "@storybook/addon-links": "^7.0.14",
    "@storybook/addon-mdx-gfm": "^7.0.14",
    "@storybook/addon-viewport": "^7.0.14",
    "@storybook/blocks": "^7.0.14",
    "@storybook/react": "^7.0.14",
    "@storybook/react-vite": "^7.0.14",
    "@storybook/testing-library": "^0.1.0",
    "@swc/core": "^1.3.59",
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitejs/plugin-react-swc": "^3.3.1",
    "autoprefixer": "^10.4.14",
    "camelcase": "^6.3.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "jsdom": "^22.0.0",
    "postcss": "^8.4.23",
    "prettier": "2.8.8",
    "prop-types": "^15.8.1",
    "rimraf": "^5.0.1",
    "storybook": "^7.0.14",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.8"
  }
}
